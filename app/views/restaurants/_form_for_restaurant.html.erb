<%= form_for(@restaurant) do |f| %>
<%= f.label :name "Enter restaurant name" %>
<%= f.text_field :name %>
<%= f.label :category "Enter restaurant category" %>
<%= f.text_field :category %>
<%= f.label :address "Enter restaurant address" %>
<%= f.text_field :address %>
<%= f.label :phone_number "Enter restaurant phone number" %>
<%= f.text_field :phone_number %>
<%= submit class = "btn btn-primary" %>
<% end %>
